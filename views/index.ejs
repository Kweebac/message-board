<!DOCTYPE html>
<html>
  <head>
    <title>Message board</title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1>Message board</h1>
    <a href="/new">New message</a>
    <% messages.map((message) => { %>
    <div>
      <strong><%= message.author %></strong>
      <p><%= message.message %></p>
      <div><%= message.dateSent %></div>
    </div>
    <% }) %>
  </body>
</html>
